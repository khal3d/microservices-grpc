{
  "require": {
    "spiral/php-grpc": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "": "gen/"
    }
  },
  "scripts": {
    "spiral-gen-proto": "protoc --php-grpc_out=./gen --php_out=./gen ../protos/service.proto --proto_path=../protos",
    "run-grpc-server": "rr-grpc serve -v -d",
    "grpc-reset": "rr-grpc grpc:reset",
    "grpc-workers": "rr-grpc grpc:workers -i"
  },
  "config": {
    "process-timeout": 0
  }
}
