{
    "require": {
        "google/protobuf": "^3.12",
        "grpc/grpc": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "": "gen",
            "App\\": "./src"
        }
    },
    "scripts": {
        "gen-proto": "protoc --grpc_out=./gen --php_out=./gen ../protos/service.proto --proto_path=../protos --plugin=protoc-gen-grpc=/usr/local/bin/grpc_php_plugin"
    }
  }
